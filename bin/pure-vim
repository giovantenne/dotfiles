#!/usr/bin/env bash
sudo apt-get update
sudo apt-get install git-core 
git clone git://github.com/zener79/dotfiles.git ~/.dotfiles
sudo apt-get install tmux vim silversearcher-ag 
sudo apt-get install gvim
ln -sf ~/.dotfiles/link/.vimrc ~/
ln -sf ~/.dotfiles/link/.vim ~/
ln -sf ~/.dotfiles/link/.tmux.conf ~/
source ~/.dotfiles/init/50_vim.sh
